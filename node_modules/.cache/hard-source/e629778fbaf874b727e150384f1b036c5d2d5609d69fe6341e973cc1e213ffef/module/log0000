ing('base64', 0, 3 - this.lastNeed);\n  return r;\n}\n\n// Pass bytes on through for single-byte encodings (e.g. ascii, latin1, hex)\nfunction simpleWrite(buf) {\n  return buf.toString(this.encoding);\n}\n\nfunction simpleEnd(buf) {\n  return buf && buf.length ? this.write(buf) : '';\n}","name":"../../../AppData/Roaming/nvm/v8.11.3/node_modules/embark/node_modules/string_decoder/lib/string_decoder.js"}},"hash":"af7fcc816df70b29cf3264416162a12f","dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[1134,1147],"loc":{"start":{"line":22,"column":0},"end":{"line":22,"column":13}}},{"type":"CommonJsRequireDependency","request":"safe-buffer","range":[1189,1202],"loc":{"start":{"line":26,"column":13},"end":{"line":26,"column":35}}},{"type":"RequireHeaderDependency","range":[1181,1188],"loc":{"start":{"line":26,"column":13},"end":{"line":26,"column":35}}}],"variables":[],"blocks":[]},"source":{"_cachedSource":{"source":{"type":"ReplaceSource","replacements":[[1134,1146,"",0],[1189,1201,"3",1],[1181,1187,"__webpack_require__",2]]},"hash":"af7fcc816df70b29cf3264416162a12f"}}}{"type":"NormalModule","constructor":{"request":"../../../AppData/Roaming/nvm/v8.11.3/node_modules/embark/node_modules/ipfs-api/src/utils/send-files-stream.js","userRequest":"../../../AppData/Roaming/nvm/v8.11.3/node_modules/embark/node_modules/ipfs-api/src/utils/send-files-stream.js","rawRequest":"../utils/send-files-stream","loaders":[],"resource":"../../../AppData/Roaming/nvm/v8.11.3/node_modules/embark/node_modules/ipfs-api/src/utils/send-files-stream.js","parser":{"type":"Parser"}},"identifier":"../../../AppData/Roaming/nvm/v8.11.3/node_modules/embark/node_modules/ipfs-api/src/utils/send-files-stream.js","assigned":{"issuer":"../../../AppData/Roaming/nvm/v8.11.3/node_modules/embark/node_modules/ipfs-api/src/files/add.js"},"build":{"built":true,"buildTimestamp":1534684002865,"cacheable":true,"meta":{},"assets":[],"fileDependencies":["../../../