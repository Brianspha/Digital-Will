rain);\n\n    source.removeListener('end', onend);\n    source.removeListener('close', onclose);\n\n    source.removeListener('error', onerror);\n    dest.removeListener('error', onerror);\n\n    source.removeListener('end', cleanup);\n    source.removeListener('close', cleanup);\n\n    dest.removeListener('close', cleanup);\n  }\n\n  source.on('end', cleanup);\n  source.on('close', cleanup);\n\n  dest.on('close', cleanup);\n\n  dest.emit('pipe', source);\n\n  // Allow for unix-like usage: A.pipe(B).pipe(C)\n  return dest;\n};\n","name":"../../../AppData/Roaming/nvm/v8.11.3/node_modules/embark/node_modules/stream-browserify/index.js"}},"hash":"be5fa6e64886783e049a26427be6ddb7","dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"CommonJsRequireDependency","request":"events","range":[1177,1185],"loc":{"start":{"line":24,"column":9},"end":{"line":24,"column":26}}},{"type":"RequireHeaderDependency","range":[1169,1176],"loc":{"start":{"line":24,"column":9},"end":{"line":24,"column":26}}},{"type":"CommonJsRequireDependency","request":"inherits","range":[1224,1234],"loc":{"start":{"line":25,"column":15},"end":{"line":25,"column":34}}},{"type":"RequireHeaderDependency","range":[1216,1223],"loc":{"start":{"line":25,"column":15},"end":{"line":25,"column":34}}},{"type":"CommonJsRequireDependency","request":"readable-stream/readable.js","range":[1286,1315],"loc":{"start":{"line":28,"column":18},"end":{"line":28,"column":56}}},{"type":"RequireHeaderDependency","range":[1278,1285],"loc":{"start":{"line":28,"column":18},"end":{"line":28,"column":56}}},{"type":"CommonJsRequireDependency","request":"readable-stream/writable.js","range":[1344,1373],"loc":{"start":{"line":29,"column":18},"end":{"line":29,"column":56}}},{"type":"RequireHeaderDependency","range":[1336,1343],"loc":{"start":{"line":29,"column":18},"end":{"line":29,"column":56}}},{"type":"CommonJsRequireDependency","request":"readable-stream/duplex.js","range":[1400,1427],"loc":{"start":{"line":30,"colum