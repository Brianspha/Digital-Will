nd, path)\n  return (file, options, _callback) => {\n    const callback = once(_callback)\n    const stream = sendFilesStream(options)\n    const concat = ConcatStream((results) => callback(null, results))\n    stream.once('error', callback)\n    stream.pipe(concat)\n    stream.write(file)\n    stream.end()\n  }\n}\n","name":"../../../AppData/Roaming/nvm/v8.11.3/node_modules/embark/node_modules/ipfs-api/src/utils/send-one-file-multiple-results.js"}},"hash":"3be13dee4f639fc44f401b03039dab47","dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,12],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":12}}},{"type":"CommonJsRequireDependency","request":"once","range":[35,41],"loc":{"start":{"line":3,"column":13},"end":{"line":3,"column":28}}},{"type":"RequireHeaderDependency","range":[27,34],"loc":{"start":{"line":3,"column":13},"end":{"line":3,"column":28}}},{"type":"CommonJsRequireDependency","request":"concat-stream","range":[72,87],"loc":{"start":{"line":4,"column":21},"end":{"line":4,"column":45}}},{"type":"RequireHeaderDependency","range":[64,71],"loc":{"start":{"line":4,"column":21},"end":{"line":4,"column":45}}},{"type":"CommonJsRequireDependency","request":"./send-files-stream","range":[121,142],"loc":{"start":{"line":5,"column":24},"end":{"line":5,"column":54}}},{"type":"RequireHeaderDependency","range":[113,120],"loc":{"start":{"line":5,"column":24},"end":{"line":5,"column":54}}}],"variables":[],"blocks":[]},"source":{"_cachedSource":{"source":{"type":"ReplaceSource","replacements":[[0,11,"",0],[35,40,"12",1],[27,33,"__webpack_require__",2],[72,86,"35",3],[64,70,"__webpack_require__",4],[121,141,"38",5],[113,119,"__webpack_require__",6]]},"hash":"3be13dee4f639fc44f401b03039dab47"}}}{"type":"NormalModule","constructor":{"request":"../../../AppData/Roaming/nvm/v8.11.3/node_modules/embark/node_modules/stream-browserify/index.js","userRequest":"../../../AppData/Roaming/nvm/v8.11.3/node_module