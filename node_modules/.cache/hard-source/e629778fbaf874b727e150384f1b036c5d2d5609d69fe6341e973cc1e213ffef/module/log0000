n      // go ahead and set it now.\n      var prev = walker.prev\n      del(self, walker)\n      walker = prev\n    }\n  }\n}\n\nfunction del (self, node) {\n  if (node) {\n    var hit = node.value\n    if (self[DISPOSE]) {\n      self[DISPOSE](hit.key, hit.value)\n    }\n    self[LENGTH] -= hit.length\n    self[CACHE].delete(hit.key)\n    self[LRU_LIST].removeNode(node)\n  }\n}\n\n// classy, since V8 prefers predictable objects.\nfunction Entry (key, value, length, now, maxAge) {\n  this.key = key\n  this.value = value\n  this.length = length\n  this.now = now\n  this.maxAge = maxAge || 0\n}\n","name":"../../../AppData/Roaming/nvm/v8.11.3/node_modules/embark/node_modules/lru-cache/index.js"}},"hash":"682c0a216dda05408b327319a20ea8a6","dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,12],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":12}}},{"type":"CommonJsRequireDependency","request":"pseudomap","range":[175,186],"loc":{"start":{"line":7,"column":10},"end":{"line":7,"column":30}}},{"type":"RequireHeaderDependency","range":[167,174],"loc":{"start":{"line":7,"column":10},"end":{"line":7,"column":30}}},{"type":"CommonJsRequireDependency","request":"util","range":[207,213],"loc":{"start":{"line":8,"column":11},"end":{"line":8,"column":26}}},{"type":"RequireHeaderDependency","range":[199,206],"loc":{"start":{"line":8,"column":11},"end":{"line":8,"column":26}}},{"type":"CommonJsRequireDependency","request":"yallist","range":[291,300],"loc":{"start":{"line":11,"column":14},"end":{"line":11,"column":32}}},{"type":"RequireHeaderDependency","range":[283,290],"loc":{"start":{"line":11,"column":14},"end":{"line":11,"column":32}}}],"variables":[],"blocks":[]},"source":{"_cachedSource":{"source":{"type":"ReplaceSource","replacements":[[0,11,"",0],[175,185,"391",1],[167,173,"__webpack_require__",2],[207,212,"20",3],[199,205,"__webpack_require__",4],[291,299,"393",5],[283,289,"__webpack_require__",6]]}