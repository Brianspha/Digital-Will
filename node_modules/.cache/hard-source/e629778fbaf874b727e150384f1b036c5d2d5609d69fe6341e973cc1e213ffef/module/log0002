   existing.forEach((m) => this.delete(m))\n    fresh.forEach((m) => this.add(m))\n  }\n\n  clear () {\n    this._multiaddrs = []\n  }\n\n  // this only really helps make ip6 and ip4 multiaddrs distinct if they are\n  // different\n  // TODO this is not an ideal solution, probably this code should just be\n  // in libp2p-tcp\n  distinct () {\n    return uniqBy(this._multiaddrs, (ma) => {\n      return [ma.toOptions().port, ma.toOptions().transport].join()\n    })\n  }\n}\n\nmodule.exports = MultiaddrSet\n","name":"../../../AppData/Roaming/nvm/v8.11.3/node_modules/embark/node_modules/peer-info/src/multiaddr-set.js"}},"hash":"547d8a7c4c980416f8d7a4aa796614b6","dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,12],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":12}}},{"type":"CommonJsRequireDependency","request":"./utils","range":[46,55],"loc":{"start":{"line":3,"column":24},"end":{"line":3,"column":42}}},{"type":"RequireHeaderDependency","range":[38,45],"loc":{"start":{"line":3,"column":24},"end":{"line":3,"column":42}}},{"type":"CommonJsRequireDependency","request":"lodash.uniqby","range":[96,111],"loc":{"start":{"line":4,"column":15},"end":{"line":4,"column":39}}},{"type":"RequireHeaderDependency","range":[88,95],"loc":{"start":{"line":4,"column":15},"end":{"line":4,"column":39}}}],"variables":[],"blocks":[]},"source":{"_cachedSource":{"source":{"type":"ReplaceSource","replacements":[[0,11,"",0],[46,54,"199",1],[38,44,"__webpack_require__",2],[96,110,"496",3],[88,94,"__webpack_require__",4]]},"hash":"547d8a7c4c980416f8d7a4aa796614b6"}}}{"type":"NormalModule","constructor":{"request":"../../../AppData/Roaming/nvm/v8.11.3/node_modules/embark/node_modules/lodash.uniqby/index.js","userRequest":"../../../AppData/Roaming/nvm/v8.11.3/node_modules/embark/node_modules/lodash.uniqby/index.js","rawRequest":"lodash.uniqby","loaders":[],"resource":"../../../AppData/Roaming/nvm/v8.11.3/node_modules/emba